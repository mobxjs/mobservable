{
    "version": "2.0.10",
    "compilerOptions": {
        "target": "es5",
        "outDir": "lib",
        "sourceMap": false,
        "declaration": true,
        "module": "commonjs",
        "removeComments": false,
		"strictNullChecks": true,
		"noImplicitReturns": true,
		"noImplicitUseStrict": true,
		"noImplicitAny": false,
		"lib": [
			"ES2015.Proxy",
			"es6",
			"dom"
		]
    },
    "exclude": [
        "node_modules",
        "test",
        "lib",
        ".build"
    ]
}
